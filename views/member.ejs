    <%
    if (islogin != 'login' || grade != 'admin') {
      %>
      <script>
      alert('관리자만 이용할 수 있는 페이지 입니다');
      location.href="/";
      </script>
      <%
    }
    %>

    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
